{
  "name": "swagger-codegen",
  "version": "0.0.1",
  "scripts": {
    "serve": "ts-node server/index.ts",
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build && tsc server/*.ts --outDir lib --esModuleInterop --resolveJsonModule",
    "preview": "vite preview",
    "preinstall": "npx only-allow pnpm",
    "prepare": "husky install"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/1161906592/swagger-codegen.git"
  },
  "keywords": [
    "swagger-ui",
    "java",
    "codegen",
    "typescript"
  ],
  "author": "liuyang<1161906592@qq.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/1161906592/swagger-codegen/issues"
  },
  "homepage": "https://github.com/1161906592/swagger-codegen#readme",
  "dependencies": {
    "@babel/generator": "^7.17.0",
    "@babel/types": "^7.17.0",
    "axios": "^0.25.0",
    "koa": "^2.13.4",
    "koa-cors": "^0.0.16",
    "koa-static": "^5.0.0",
    "lru-cache": "^7.3.1",
    "mockjs": "^1.1.0"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.0.1",
    "@commitlint/config-conventional": "^16.0.0",
    "@types/babel__generator": "^7.6.4",
    "@types/koa": "^2.13.4",
    "@types/koa-cors": "^0.0.2",
    "@types/koa-static": "^4.0.2",
    "@types/lru-cache": "^5.1.1",
    "@types/mockjs": "^1.0.6",
    "@types/node": "^17.0.15",
    "@vicons/ionicons5": "^0.12.0",
    "@vitejs/plugin-vue": "^2.0.0",
    "@vitejs/plugin-vue-jsx": "^1.3.3",
    "axios": "^0.25.0",
    "eslint": "^8.6.0",
    "eslint-config-vue-tsx": "github:1161906592/eslint-config-vue-tsx",
    "husky": "^7.0.4",
    "lint-staged": "^12.1.5",
    "mock-server": "github:1161906592/mock-server",
    "monaco-editor": "^0.32.1",
    "naive-ui": "^2.25.2",
    "prettier": "^2.5.1",
    "sass": "^1.45.2",
    "ts-node": "^10.5.0",
    "typescript": "^4.4.4",
    "vite": "^2.7.2",
    "vue": "^3.2.26",
    "vue-tsc": "^0.29.8"
  },
  "lint-staged": {
    "*.{ts,tsx}": [
      "eslint --fix"
    ],
    "*.{css,scss,html}": [
      "prettier --write"
    ]
  }
}